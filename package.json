{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:seed": "npx tsx scripts/seed-mongodb.ts",
    "db:migrate-permissions": "npx tsx scripts/migrate-permissions.ts",
    "db:reset": "npx tsx scripts/reset-mongodb.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:unit": "jest --testPathPatterns=__tests__ --testPathIgnorePatterns=e2e --coverage",
    "test:integration": "jest --testPathPatterns=__tests__/api --runInBand",
    "test:components": "jest --testPathPatterns=components",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:prod": "npm run test:unit && npm run test:integration",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "pretest:e2e": "npm run playwright:install",
    "release": "npm run build && npm run test:all",
    "audit:security": "npm audit --audit-level moderate",
    "audit:licenses": "npx license-checker --excludePackages 'gpl-3.0;agpl-3.0' --summary",
    "analyze": "npm run type-check && npm run lint && npm run audit:security",
    "clean": "rm -rf .next out dist coverage test-results playwright-report",
    "fresh": "npm run clean && npm ci && npm run build"
  },
